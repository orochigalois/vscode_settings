// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "cmd+y",
        "command": "redo",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+k 1",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "<?php $0 ?>"
        }
    },
    {
        "key": "cmd+k 2",
        "command": "editor.action.insertSnippet",
        "when": "editorTextFocus",
        "args": {
            "snippet": "<?= $0; ?>"
        }
    }
]